<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="jp.co.nichiwa_system.chugengiyuya.myapplication.MainActivity3Activity">





    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="備蓄品"
        android:id="@+id/textView17"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/button2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button2"
        android:layout_toEndOf="@+id/button2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:id="@+id/button10"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button9" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="設定"
        android:id="@+id/button11"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button10" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button12"
        android:src="@drawable/gas"
        android:layout_below="@+id/textView17"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button13"
        android:src="@drawable/matti"
        android:layout_alignTop="@+id/button12"
        android:layout_toEndOf="@+id/button12" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button14"
        android:src="@drawable/bombe"
        android:layout_above="@+id/button15"
        android:layout_toEndOf="@+id/textView17" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button15"
        android:src="@drawable/hue"
        android:layout_below="@+id/button12"
        android:layout_toStartOf="@+id/button13"
        android:layout_marginTop="29dp" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button16"
        android:src="@drawable/sitagi"
        android:layout_alignTop="@+id/button15"
        android:layout_alignStart="@+id/button13" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button17"
        android:src="@drawable/thissyu"
        android:layout_alignTop="@+id/button16"
        android:layout_alignStart="@+id/button14" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button18"
        android:layout_marginTop="32dp"
        android:src="@drawable/almi"
        android:layout_below="@+id/button15"
        android:layout_toStartOf="@+id/button9" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/button19"
        android:src="@drawable/gunnte"
        android:layout_alignTop="@+id/button18"
        android:layout_alignStart="@+id/button16" />


</RelativeLayout>
