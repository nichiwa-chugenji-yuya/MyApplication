<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="jp.co.nichiwa_system.chugengiyuya.myapplication.MainActivity2Activity">

    <Button
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/button5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:id="@+id/button6"
        android:layout_marginEnd="27dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button7" />

    <Button
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:id="@+id/button7"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button8"
        android:layout_marginEnd="31dp" />

    <Button
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:text="設定"
        android:id="@+id/button8"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="大人"
        android:id="@+id/textView5"
        android:layout_marginTop="93dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/button6" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:inputType="number"
        android:maxLength="2"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="人"
        android:id="@+id/textView7"
        android:layout_alignTop="@+id/textView5"
        android:layout_toEndOf="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="小人"
        android:id="@+id/textView8"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:maxLength="2"
        android:id="@+id/textView9"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="人"
        android:id="@+id/textView10"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="期日"
        android:id="@+id/textView11"
        android:layout_alignTop="@+id/textView12"
        android:layout_toStartOf="@+id/textView12" />

    <EditText
        android:maxLength="2"
        android:inputType="number"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:layout_marginTop="58dp"
        android:layout_below="@+id/textView9"
        android:layout_toStartOf="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="日"
        android:id="@+id/textView13"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="設定"
        android:id="@+id/textView14"
        android:layout_marginTop="53dp"
        android:layout_below="@+id/textView12"
        android:layout_toStartOf="@+id/textView12" />

    <EditText
        android:maxLength="2"
        android:inputType="number"
        android:layout_width="50dp"

        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_alignBottom="@+id/textView14"
        android:layout_toEndOf="@+id/textView14" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="日"
        android:id="@+id/textView16"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignStart="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="幼児"
        android:id="@+id/textView34"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignStart="@+id/button7" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:maxLength="2"
        android:inputType="number"
        android:id="@+id/textView35"
        android:layout_alignTop="@+id/textView6"
        android:layout_toEndOf="@+id/textView34" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="人"
        android:id="@+id/textView36"
        android:layout_alignTop="@+id/textView34"
        android:layout_alignStart="@+id/button8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="※中学生以上"
        android:id="@+id/textView37"
        android:layout_above="@+id/textView9"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="※2歳以下"
        android:id="@+id/textView38"
        android:layout_alignTop="@+id/textView37"
        android:layout_toEndOf="@+id/button6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="3歳以上中学生未満"
        android:id="@+id/textView39"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="※消費期限到達日"
        android:id="@+id/textView40"
        android:layout_above="@+id/textView15"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="※何日分の備蓄を設定"
        android:id="@+id/textView41"
        android:layout_below="@+id/textView14"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp" />

</RelativeLayout>
